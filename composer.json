{
    "name": "atoum/reports-extension",
    "type": "library",
    "description": "atoum Reports extension",
    "keywords": ["TDD","atoum","test","unit testing","reports"],
    "homepage": "http://www.atoum.org",
    "license": "BSD",
    "authors": [
        {
            "name": "jubianchi",
            "email": "contact@jubianchi.fr"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jubianchi/atoum"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "atoum/atoum": "dev-runner-auto-conf@dev",
        "twig/twig": "^1.18",
        "symfony/filesystem": "^2.6 | ^3.0"
    },
    "autoload": {
        "psr-4": {
            "mageekguy\\atoum\\reports\\": "classes"
        },
        "files": [
            "autoloader.php",
            "configuration.php"
        ]
    },
    "autoload-dev": {
        "files": ["tests/autoloader.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
